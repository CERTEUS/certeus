# Enterprise Environment Variables
# These should be set in GitHub Secrets for production

# Security
POSTGRES_PASSWORD=<strong-random-password>
MINIO_SECRET_KEY=<strong-random-key>
RELEASE_PLEASE_TOKEN=<github-personal-access-token>

# Enterprise Features
ENTERPRISE_MODE=true
AUTO_MERGE_ENABLED=true
SECURITY_SCAN_ENABLED=true

# Monitoring
PROMETHEUS_ENDPOINT=<prometheus-url>
GRAFANA_ENDPOINT=<grafana-url>

# Notifications
SLACK_WEBHOOK_URL=<slack-webhook>
TEAMS_WEBHOOK_URL=<teams-webhook>

# Enterprise License
ENTERPRISE_LICENSE_KEY=<license-key>