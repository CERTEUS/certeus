{
  "typeCheckingMode": "standard",
  "reportMissingTypeStubs": false,
  "stubPath": "typings",
  "include": ["."],
  "exclude": [
    ".venv",
    "**/venv",
    "**/env",
    "**/__pycache__",
    "**/node_modules",
    "**/.git",
    "exports",
    "artifacts",
    "reports",
    "static/previews",
    "out",
    "proofs"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["."]
    }
  ]
}
