{
  "typeCheckingMode": "off",
  "reportMissingTypeStubs": false,
  "stubPath": "typings",
  "include": [
    "__init__.py",
    "main.py",
    "conftest.py",
    "core/api.py",
    "core/version.py",
    "core/plugin_loader.py",
    "core/pco/__init__.py",
    "core/omega/__init__.py",
    "services/api_gateway/main.py",
    "services/api_gateway/idempotency.py",
    "services/api_gateway/shedder.py",
    "services/api_gateway/routers/boundary.py",
    "services/api_gateway/routers/billing.py",
    "services/api_gateway/routers/pco.py",
    "services/api_gateway/routers/ledger.py",
    "services/api_gateway/routers/cfe_config.py",
    "services/api_gateway/routers/verify.py",
