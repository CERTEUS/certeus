{
  "typeCheckingMode": "off",
  "reportMissingTypeStubs": false,
  "stubPath": "typings",
  "include": [
    "__init__.py",
    "main.py", 
    "conftest.py"
  ],
  "exclude": [
    ".venv",
    "**/.venv",
    "**/venv",
    "**/env",
    "**/__pycache__",
    "**/node_modules",
    "**/.git",
    "core",
    "tests",
    "services",
    "scripts",
    "billing",
    "certeus",
    "cje",
    "clients",
    "exports",
    "artifacts",
    "reports",
    "static/previews",
    "out",
    "proofs"
  ],
  "executionEnvironments": [
    {
      "root": ".",
      "extraPaths": ["."]
    }
  ]
}
